{
  "@context": [
    "https://w3c.github.io/wot/w3c-wot-td-context.jsonld"
  ],
  "@type": [
    "Test Bench"
  ],
  "name": "thing_test_bench",
  "base": "http://localhost:8090/",
  "interaction": [
    {
      "@type": [
        "Property"
      ],
      "name": "testConfig",
      "outputData": {
        "type": "object",
        "properties": {
          "thingTdLocation": {
            "type": "string"
          },
          "thingTdName": {
            "type": "string"
          },
          "schemaLocation": {
            "type": "string"
          },
          "testReportsLocation": {
            "type": "string"
          },
          "requestsLocation": {
            "type": "string"
          },
          "repetitions": {
            "type": "integer",
            "minimum": 1
          },
          "maximumTestScenarios": {
            "type": "integer",
            "minimum": 1
          }
        },
        "additionalProperties": false
      },
      "writable": true,
      "link": [
        {
          "href": "testConfig",
          "mediaType": "application/json"
        }
      ]
    },
    {
      "@type": [
        "Action"
      ],
      "name": "initiate",
      "outputData": {
        "type": "boolean"
      },
      "link": [
        {
          "href": "initiate",
          "mediaType": "application/json"
        }
      ]
    },
    {
      "@type": [
        "Action"
      ],
      "name": "generateRequests",
      "inputData": {
        "type": "string",
        "enum": [
          "manual",
          "template",
          "automatic"
        ]
      },
      "link": [
        {
          "href": "generateRequests",
          "mediaType": "application/json"
        }
      ]
    },
    {
      "@type": [
        "Property"
      ],
      "name": "requests",
      "outputData": {
        "type": "array"
      },
      "writable": true,
      "link": [
        {
          "href": "requests",
          "mediaType": "application/json"
        }
      ]
    },
    {
      "@type": [
        "Action"
      ],
      "name": "testThing",
      "inputData": {
        "type": "boolean"
      },
      "outputData": {
        "type": "boolean"
      },
      "link": [
        {
          "href": "testThing",
          "mediaType": "application/json"
        }
      ]
    },
    {
      "@type": [
        "Property"
      ],
      "name": "testReport",
      "outputData": {
        "type": "array"
      },
      "writable": false,
      "link": [
        {
          "href": "testReport",
          "mediaType": "application/json"
        }
      ]
    }
  ]
}